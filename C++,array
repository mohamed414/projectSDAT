int main()
{
int arr [4][4] ;
for(int row = 0 ; row < 4 ; row++){
for(int coulmn = 0 ; coulmn < 4 ; coulmn ++){
 cin>> arr[row][coulmn];
}
}
for(int row = 0 ; row < 4 ; row++){
for(int coulmn = 0 ; coulmn < 4 ; coulmn ++){
 cout<< arr[row][coulmn] << "\t"  ;
}
cout<<endl ;
}
cout<<endl ;
return 0 ;
}
